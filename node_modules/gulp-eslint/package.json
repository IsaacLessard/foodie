{
  "name": "gulp-eslint",
  "version": "1.1.0",
  "description": "A gulp plugin for processing files with ESLint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adametry/gulp-eslint.git"
  },
  "files": [
    "index.js",
    "util.js"
  ],
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "pretest": "jscs *.js test/*.js && gulp test",
    "test": "mocha",
    "gulp": "gulp",
    "gulp-example": "gulp --gulpfile=example/config.js",
    "coverage": "istanbul cover _mocha",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "keywords": [
    "gulpplugin",
    "eslint",
    "gulp",
    "errors",
    "warnings",
    "check",
    "source",
    "code",
    "formatter",
    "js",
    "javascript",
    "task",
    "lint",
    "plugin"
  ],
  "author": {
    "name": "Adametry"
  },
  "contributors": [
    {
      "name": "Shinnosuke Watanabe",
      "email": "snnskwtnb@gmail.com",
      "url": "https://github.com/shinnn"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "bufferstreams": "^1.1.0",
    "eslint": "^1.4.0",
    "gulp-util": "^3.0.6",
    "object-assign": "^4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.5",
    "gulp": "^3.9.0",
    "istanbul": "^0.4.0",
    "istanbul-coveralls": "^1.0.3",
    "jscs": "^2.0.0",
    "mocha": "^2.2.5",
    "should": "^7.0.1",
    "vinyl": "^1.0.0"
  },
  "jscsConfig": {
    "preset": "google",
    "validateIndentation": "\t",
    "disallowMultipleVarDecl": null,
    "maximumLineLength": {
      "value": 98,
      "allowComments": true
    }
  },
  "gitHead": "b00c579a01b52597756eb7b9d45b1926adc611ef",
  "bugs": {
    "url": "https://github.com/adametry/gulp-eslint/issues"
  },
  "homepage": "https://github.com/adametry/gulp-eslint#readme",
